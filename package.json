{
  "name": "tribe-mvp",
  "version": "0.1.0",
  "private": true,
  "description": "Smart baby update distribution platform",
  "author": "Colin Rodrigues",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "build:fast": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=/__tests__/.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=integration",
    "test:responses": "jest --testPathPattern=responses",
    "test:a11y": "jest --testPathPattern=a11y",
    "test:supabase": "node scripts/test-supabase.js",
    "test:email": "node scripts/test-email.js",
    "check:contrast": "tsx scripts/check-color-contrast.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:generate-types": "supabase gen types typescript --local > lib/types/database.types.ts",
    "supabase:seed": "supabase db reset && node -e \"console.log('Run: supabase db reset --db-url=your-db-url')\"",
    "db:seed": "psql $DATABASE_URL -f supabase/seed/test_data.sql",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:debug": "playwright test --debug",
    "test:visual:report": "playwright show-report"
  },
